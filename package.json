{
  "name": "electron-fd",
  "productName": "flowdock",
  "version": "0.1.1",
  "main": "main.js",
  "devDependencies": {
    "electron-packager": "^7.7.0",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.6.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0"
  },
  "dependencies": {
    "electron": "^1.4.3",
    "simple-spellchecker": "^0.9.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/electron main.js",
    "build": "./node_modules/.bin/electron-packager . --out=dist --prune --asar --overwrite --platform=all --arch=x64",
    "build_linux": "./node_modules/.bin/electron-packager . --out=dist --prune --asar --overwrite --platform=linux --arch=x64"
  }
}
